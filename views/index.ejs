<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script type="text/javascript" src="/javascripts/jquery-1.10.2.min.js" ></script>
    <script type="text/javascript" src="/javascripts/jquery.cycle.all.js" ></script>
    <script type="text/javascript" src="/javascripts/jquery.touchwipe.1.1.1.js" ></script>

      <style type="text/css">
          .d1 {
              position: absolute;
              top: 50px;
              left: 300px;
              display: none;
              z-index: 3;
              opacity: 1;
              width: 200px;
              height: 200px;

          }
      </style>
  </head>
  <body>
    <div id="main" style="width: 500px;height: 600px;border:3px solid darkred">
        <div class="d1" style="background: red">1</div>
        <div class="d1" style="background: green">2</div>
        <div class="d1" style="background: blue">3</div>
    </div>

  <script type="text/javascript">

      $('#main').cycle({
          timeout: 0,
          fx: 'scrollHorz'
      });


      $("#main").touchwipe({
          wipeLeft: function() {
              $("#main").cycle("next");
          },
          wipeRight: function() {
              $("#main").cycle("prev");
          },
          min_move_x: 80,
          min_move_y: 80,
          preventDefaultEvents: true
      });
  </script>
  </body>
</html>